{
  "name": "zhishi",
  "version": "1.0.0",
  "description": "A question and answer app for Andela fellows",
  "main": "app/js/index.js",
  "author": "",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:nolint": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* app tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"app/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "rimraf coverage/ && istanbul cover _mocha tools/testSetup.js \"app/**/*.spec.js\"",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --port 8000",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "copy-assets": "cp -r build/assets dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test copy-assets build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "dependencies": {
    "body-parser": "^1.4.3",
