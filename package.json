    "eslint-plugin-react": "^3.6.3",
    "expect": "1.19.0",
    "express": "^4.13.3",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^0.8.4",
    "flux": "^2.0.1",
    "history": "^1.12.5",
